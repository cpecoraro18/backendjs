{
  "version": "0.9.0",
  "author": "Vlad Seryakov",
  "name": "backendjs",
  "description": "Backend.js framework",
  "main": "index",
  "preferGlobal": "true",
  "homepage": "https://github.com/vseryakov/backendjs",
  "repository": {
    "type": "git",
    "url": "https://github.com/vseryakov/backendjs.git"
  },
  "dependencies": {
    "uuid": ">= 1.4.1",
    "printf" : ">= 0.2.0",
    "node-expat": ">= 2.1.4",
    "async": ">= 0.6.2",
    "cron": ">= 1.0.4",
    "express": ">= 3.5.1",
    "generic-pool": ">= 2.0.4",
    "emailjs": ">= 0.3.6",
    "cheerio": ">= 0.14.0",
    "xml2json": ">= 0.4.0",
    "mime": ">= 1.2.11",
    "marked": ">= 0.3.2",
    "http-proxy": ">= 1.0.3",
    "memcached": ">= 0.2.8",
    "redis": ">= 0.7.1",
    "qs": ">= 0.6.6",
    "toobusy": ">= 0.2.4",
    "helenus": ">= 0.6.9",
    "connect": ">= 2.14.3",
    "formidable": ">= 1.0.14",
    "amqp": ">= 0.2.0",
    "consolidate": ">= 0.10.0",
    "cookie-parser": ">= 1.0.1",
    "cookie-session": ">= 1.0.1",
    "serve-static": ">= 1.0.1"
  },
  "keywords": [
    "database",
    "API",
    "DynamoDB",
    "Cassandra",
    "Sqlite",
    "PostgreSQL",
    "MySQL",
    "account",
    "location",
    "instance",
    "jobs",
    "cron",
    "geohash"
  ],
  "engines": {
    "node": ">= 0.10"
  },
  "license": "BSD-3-Clause",
  "bin": {
    "rc.backend": "./rc.backend"
  },
  "scripts": {
    "preinstall": "([ ! -d build/include ] && ./rc.backend build-deps) || (exit 0)",
    "install": "node-gyp configure build",
    "postinstall": "([ -f build/Release/backend.node ] && mv build/Release/backend.node build) || (exit 0)",
    "start": "rc.backend run-backend"
  }
}
